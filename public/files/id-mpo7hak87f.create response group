
# import ของ
from response.models import ResponseGroups
from user.models import ResponseUserGroups
from user.models import AuthUser
from survey.models import Survey


# สร้าง group ใน survey slug = 'col-col-im'
survey =Survey.objects.get(slug='col-col-im')
ResponseGroups.objects.create(slug='a1', name='Thailand', survey=survey)
ResponseGroups.objects.create(slug='b1', name='ChiangMai', parent_group_slug='a1',  survey=survey)
ResponseGroups.objects.create(slug='c1', name='Saraphi', parent_group_slug='b1',  survey=survey)
ResponseGroups.objects.create(slug='c2', name='Hangdong', parent_group_slug='b1',  survey=survey)
ResponseGroups.objects.create(slug='b2', name='Bangkok', parent_group_slug='a1',  survey=survey)


ResponseGroups.objects.create(slug='x1', name='USA', survey=survey)
ResponseGroups.objects.create(slug='y1', name='New York', parent_group_slug='x1',  survey=survey)
ResponseGroups.objects.create(slug='y2', name='Lost Angeles', parent_group_slug='x1',  survey=survey)
ResponseGroups.objects.create(slug='y3', name='San Diego', parent_group_slug='x1',  survey=survey)

# เลือก user ที่สร้างใน admin
auth = AuthUser.objects.get(username='a1')


# เพิ่ม user ที่เลือกลงกลุ่ม b1 ถ้าดูจากต้านบนคือ chiang mai
ResponseUserGroups.objects.create(authuser=auth, group_slug='b1')

